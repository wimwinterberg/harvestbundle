<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="we_are_builders_harvest.service.class">WeAreBuilders\HarvestBundle\Services\Harvest</parameter>
        <parameter key="we_are_builders_harvest.user">null</parameter>
        <parameter key="we_are_builders_harvest.password">null</parameter>
        <parameter key="we_are_builders_harvest.account">null</parameter>
    </parameters>

    <services>
        <service id="wrb_harvest" class="%we_are_builders_harvest.service.class%">
            <argument key="user">%we_are_builders_harvest.user%</argument>
            <argument key="password">%we_are_builders_harvest.password%</argument>
            <argument key="account">%we_are_builders_harvest.account%</argument>
        </service>
    </services>
</container>